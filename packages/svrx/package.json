{
  "name": "@svrx/svrx",
  "version": "1.1.5",
  "description": "svrx server live-reload mock ",
  "keywords": [
    "svrx",
    "server",
    "live-reload",
    "mock"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://svrx.io/",
  "license": "MIT",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "resource",
    "npm-shrinkwrap.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/svrxjs/svrx"
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text mocha --recursive __tests__/spec --exit",
    "webpack:dev": "webpack --config ./scripts/webpack.injector.js -w",
    "webpack:build": "webpack --config ./scripts/webpack.injector.js",
    "dev": "npm run webpack:dev",
    "build": "npm run webpack:build",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "dependencies": {
    "@svrx/util": "file:../svrx-util",
    "ajv-errors": "^1.0.1",
    "chalk": "^3.0.0",
    "chokidar": "^3.1.1",
    "co-request": "^1.0.0",
    "find-free-port": "^2.0.0",
    "immer": "^8.0.4",
    "koa": "^2.7.0",
    "koa-history-api-fallback": "^1.0.0",
    "koa-send": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.11",
    "micromatch": "^4.0.2",
    "path-to-regexp": "^6.1.0",
    "raw-body": "^2.4.0",
    "requires-port": "^1.0.0",
    "resolve": "^1.10.0",
    "semver": "^7.1.1",
    "serve-index": "^1.9.1",
    "socket.io": "^2.3.0",
    "super-ajv": "^1.1.6"
  },
  "devDependencies": {
    "koa-bodyparser": "^4.2.1",
    "node-easy-cert": "^1.3.1",
    "qwest": "^4.5.0",
    "socket.io-client": "^2.3.0"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
